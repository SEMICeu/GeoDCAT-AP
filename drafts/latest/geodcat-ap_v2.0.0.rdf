  <rdf:Description rdf:about="http://data.europa.eu/930/spatialResolutionAsScale">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/> 
    <rdfs:label xml:lang="en">spatial resolution as equivalent scale</rdfs:label>
    <rdfs:comment xml:lang="en">Spatial resolution of a dataset expressed as equivalent scale, measured by using a representative fraction (e.g., 1:1,000, 1:1,000,000).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://data.europa.eu/930/spatialResolutionInDegrees">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/> 
    <rdfs:label xml:lang="en">spatial resolution as angular distance</rdfs:label>
