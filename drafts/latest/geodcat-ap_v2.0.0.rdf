<!-- Spatial resolution -->

  <rdf:Description rdf:about="http://data.europa.eu/930/spatialResolutionAsScale">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/> 
    <rdfs:label xml:lang="en">spatial resolution as equivalent scale</rdfs:label>
    <rdfs:comment xml:lang="en">Spatial resolution of a dataset expressed as equivalent scale, measured by using a representative fraction (e.g., 1:1,000, 1:1,000,000).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://data.europa.eu/930/spatialResolutionInDegrees">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/> 
    <rdfs:label xml:lang="en">spatial resolution as angular distance</rdfs:label>
    <rdfs:comment xml:lang="en">Spatial resolution of a dataset expressed as angular distance, measured in decimal degrees.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://data.europa.eu/930/spatialResolutionAsVerticalDistanceInMeters">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/> 
    <rdfs:label xml:lang="en">spatial resolution as equivalent scale</rdfs:label>
    <rdfs:comment xml:lang="en">Spatial resolution of a dataset expressed as vertical distance, measured in metres.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </rdf:Description>
  
<!-- Agent roles -->
  
  <rdf:Description rdf:about="http://data.europa.eu/930/custodian">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">Custodian</rdfs:label>
    <rdfs:comment xml:lang="en">Party that accepts accountability and responsibility for the data and ensures appropriate care and maintenance of the resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/eli/reg/2008/1205/2008-12-24"/>
    <rdfs:isDefinedBy rdf:resource="https://www.iso.org/standard/26020.html"/>
    <rdfs:isDefinedBy rdf:resource="https://www.iso.org/standard/53798.html"/>
    <rdfs:seeAlso rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/ResponsiblePartyRole/custodian"/>
  </rdf:Description>
  
